{
    "manifest_version": 3,
    "name": "Tab Counter",
    "description": "Counts number of tabs open in window",
    "version": "1.0",
    "permissions": ["tabs", "background"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": "smirk.png"
    },
    "background": {
        "service_worker": "static/tabs.js"
    }
  }